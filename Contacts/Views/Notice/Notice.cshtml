@model IEnumerable<Contacts.Models.NoticeModel>

@{
    ViewBag.Title = "Уведомление";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="jumbotron">

    @if (Model.Any())
    {
        foreach (var s in Model)
        {
            <h1>@s.Text</h1>
        }
    }
    else
    {
        <h1>Сегодня уведомлений нет!</h1>
    }

</div>

