@model IEnumerable<Contacts.Models.HolidayModel>

@{
    ViewBag.Title = "Праздники";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Праздники</h2>

<table class="table">
    <tr>
        <th>
            Название праздника:
        </th>
        <th>
            Дата праздника:
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @item.DateHoliday.ToString("dd-MM")
        </td>
    </tr>
}

</table>
